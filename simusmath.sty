% multiset with <>
% by chrigu
\newcommand{\multiset}[2]{\left<\begin{array}{c}#1\\#2\end{array}\right>}

% multiset as fraction using rising factorial
\newcommand{\multisetii}[2]{\frac{[#1]^{#2}}{(#2)!}}

% multiset as fraction using rising factorial, with parenthesis around denominator
\newcommand{\multiseti}[2]{\frac{[#1]^{#2}}{#2!}}
